<h1>Add Photo Shoot</h1>
<%= form_for @shoot do |f| %>
	<%= f.label :name %>
  <%= f.text_field :name %> 
  
  <%= f.label :email %>
  <%= f.text_field :email %>
    <%= f.label :phone %>
  <%= f.text_field :phone %><br />
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :time %>
  <%= f.text_field :time %><br />

  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %><br />
    <%= f.label :description %>
  <%= f.text_area :description %><br />
  <div class="btn-group">
 <%= f.button :submit, :class => "btn btn-inverse" %>
 </div>
 
	<% end %>


