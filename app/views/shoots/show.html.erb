<h1>Manage Photo Shoot Details</h1>
<%= form_for @shoots do |f| %>
	<%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
    <%= f.label :phone %>
  <%= f.text_field :phone %><br />
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :time %>
  <%= f.text_field :time %><br />

  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %><br />
    <%= f.label :description %>
  <%= f.text_area :description %><br />
  
 



	<%= f.submit "edit Shoot", url: shoot_path, :method => :put %><br />
	<%= f.submit "Delete Shoot", url: shoot_path, :method => :delete %><br />

	<%= link_to("Back to Main Page", {:controller => "home", :action => "index" }) %><br />

	<%= link_to("Back to Shoots Page", {:controller => "shoots", :action => "index" }) %>

	<%#= button_to "delete", @shoot, :method => :delete %>
	<% end %>