<h1>Add Camera Gear</h1>
<%= form_for @equipment do |f| %>
	<%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :serial %>
  <%= f.text_field :serial %>
 

	<%= f.submit "Add Gear" %>
	<%= link_to("Back to Main Page", {:controller => "home", :action => "index" }) %>

	<%= link_to("Back to Equipment Page", {:controller => "equipments", :action => "index" }) %>
	<% end %>




 
