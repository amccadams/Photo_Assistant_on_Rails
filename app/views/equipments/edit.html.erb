<h1>Edit Equipment Detail</h1>
<%= form_for @equipment do |f| %>
	<%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :serial %>
  <%= f.text_field :serial %>
 



	<%= f.submit "Edit Equipment", url: equipment_path, :method => :put %>
	<%= link_to("Back to Equipment", {:controller => "equipments", :action => "index" }) %>
	<% end %>
